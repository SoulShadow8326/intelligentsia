{
  "version": 2,
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/upload-id", "dest": "/api/upload-id" },

  { "src": "^/css/(.*)$", "dest": "/frontend/css/$1" },
  { "src": "^/js/(.*)$", "dest": "/frontend/js/$1" },
  { "src": "^/assets/(.*)$", "dest": "/frontend/assets/$1" },
  { "src": "^/fonts/(.*)$", "dest": "/frontend/fonts/$1" },
  { "src": "^/good/css/(.*)$", "dest": "/good/css/$1" },
  { "src": "^/good/js/(.*)$", "dest": "/good/js/$1" },
  { "src": "^/good/assets/(.*)$", "dest": "/good/assets/$1" },

  { "src": "^/(.*\\.(?:ico|png|jpg|jpeg|svg|webp))$", "dest": "/frontend/$1" },

    { "src": "^/(.*)$", "dest": "/api/index.py" }
  ]
}
